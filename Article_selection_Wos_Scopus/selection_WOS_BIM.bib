Title = {The Practical Implications of Using Fuzzy Logic for Mapping Data for
   Life Cycle Analysis},
Booktitle = {COOPERATIVE DESIGN, VISUALIZATION, AND ENGINEERING, CDVE 2022},
Series = {Lecture Notes in Computer Science},
