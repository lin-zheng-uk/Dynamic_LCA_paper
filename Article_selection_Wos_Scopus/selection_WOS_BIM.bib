@inproceedings{ WOS:000253394500229,
Author = {Chiou, Yun-Shang and Lam, Khee Poh},
Editor = {Jiang, Y and Zhu, YX and Yang, XD and Li, XT},
Title = {Sensor-based information modeling for life cycle commissioning of
   residential buildings},
