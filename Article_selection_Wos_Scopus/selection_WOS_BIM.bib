Type = {Article},
Language = {English},
Affiliation = {Wu, J (Corresponding Author), Stanford Univ, Dept Civil \& Environm Engn, Stanford, CA 94305 USA.
   Wu, Jie; Lepech, Michael D., Stanford Univ, Dept Civil \& Environm Engn, Stanford, CA 94305 USA.},
DOI = {10.1016/j.autcon.2019.103004},
Article-Number = {103004},
ISSN = {0926-5805},
EISSN = {1872-7891},
