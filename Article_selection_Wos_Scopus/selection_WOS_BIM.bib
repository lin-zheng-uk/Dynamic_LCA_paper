Booktitle = {EWORK AND EBUSINESS IN ARCHITECTURE, ENGINEERING AND CONSTRUCTION},
Year = {2012},
Pages = {855-862},
Note = {9th European Conference on Product and Process Modelling, Reykjavik,
   ICELAND, JUL 25-27, 2012},
Abstract = {In this paper uses of building information modeling (BIM) are
   investigated in a construction project in Finland. We analyze especially
   how the uses of BIM technologies mediated collaboration among project
   partners. Three main forms of collaboration are discerned on the basis
   of interview data: 1) BIM providing information, 2) BIM models
   instigating commenting and problem solving, 3) BIM models and
   information developed further with BIM. Examples of these forms of
   collaboration are presented as well related social and technical
   challenges. At the end, the BIM collaboration paradox is outlined, that
   is, BIM technologies seem both to reduce and add the need for social
   interaction.},
