Author = {Nyvlt, Vladimir and Novotny, Radimir},
Editor = {Pruskova, K and Vochozka, M and Senitkova, IJ and Fariborz, H and Vachal, J and Kulhanek, F and Juhas, P and Marecek, J and Olah, J and Flimel, M and Melcher, J and Silaarova, S},
Title = {Sharing Knowledge and Information within BIM Life Cycle Processes},
