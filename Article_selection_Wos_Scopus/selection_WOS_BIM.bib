@article{ WOS:000970690500001,
Author = {Wang, Taige and Chen, Han-Mei},
Title = {Integration of building information modeling and project management in
   construction project life cycle},
Journal = {AUTOMATION IN CONSTRUCTION},
Year = {2023},
Volume = {150},
Month = {JUN},
Abstract = {Building Information Modeling (BIM) has spread worldwide not only as a
   digital construction tool but as a management method and is expected to
   play an essential role in project management. Before further exploring
   the contribution of BIM capabilities to project management, it is
   necessary to understand the integration of BIM and project management
   throughout the project life cycle. This paper aims to comprehensively
   understand the research status by reviewing the integration of BIM and
   project management in the construction project life cycle. The analysis
   results show that although the number of articles in this field has
   increased rapidly in the past 11 years, their relationship with the
   project life cycle is still relatively weak. To this end, a BIM
   capabilities framework is proposed by summarising BIM capabilities for
   project management in the project life cycle. This study will facilitate
   construction project teams and stakeholders to continuously integrate
   BIM with project management throughout the project life cycle, thereby
   improving project management efficiency in construction projects.},
