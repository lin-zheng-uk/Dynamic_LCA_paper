	doi = {10.1016/j.enbuild.2019.109519},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85073550743&doi=10.1016%2fj.enbuild.2019.109519&partnerID=40&md5=5d7098634e115b77b453cd66fc7a678f},
