@ARTICLE{Hu2023,
	author = {Hu, Shuling and Lei, Xiaoming},
	title = {Machine learning and genetic algorithm-based framework for the life-cycle cost-based optimal design of self-centering building structures},
	year = {2023},
	journal = {Journal of Building Engineering},
	volume = {78},
	doi = {10.1016/j.jobe.2023.107671},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85170103328&doi=10.1016%2fj.jobe.2023.107671&partnerID=40&md5=611cf8b9a3c934dd3c75003795fd38dd},
	affiliations = {JSPS International Research Fellow, Department of Architecture and Architectural Engineering, Kyoto University, Kyoto, Japan; Department of Civil and Environmental Engineering, The Hong Kong Polytechnic University, Kowloon, Hong Kong},
	abstract = {Self-centering systems have emerged as innovative lateral force-resisting mechanisms aimed at improving the earthquake resilience of building structures. The fully self-centering behavior effectively eliminates the residual deformation, thereby improving the post-earthquake repairability of building structures. However, this behavior does intensify floor acceleration responses that contribute to nonstructural damage and lead to increased initial costs. This research aims to develop a general machine learning and genetic algorithm-based framework for optimizing the design of self-centering building structures and to strike a balance between the initial costs and seismic losses by minimizing the overall life-cycle cost. The artificial neural network (ANN) based StructureNet model was developed for accelerating the calculation of life-cycle costs associated with self-centering structures. The genetic algorithm was adopted to determine the optimal design parameters for self-centering structures. The framework was applied to 8 design cases, with the analysis centering on two factors: the maximum allowable residual inter-story drift for controlling demolition and the initial construction cost of self-centering members. The study examined their influences on the optimization design results of self-centering building structures. The findings highlight a preference for a design approach that leans toward partially self-centering behavior as opposed to fully self-centering behavior, indicating the higher life-cycle benefits of partially self-centering behavior. © 2023 Elsevier Ltd},
	author_keywords = {ANN; Genetic algorithm; Life-cycle cost; Optimal design; Partially self-centering behavior; Self-centering},
	keywords = {Architectural design; Cost benefit analysis; Earthquakes; Learning algorithms; Life cycle; Machine learning; Neural networks; Optimal systems; Seismic design; Building structure; Centering behavior; Initial costs; Life-cycle cost; Machine-learning; Optimal design; Partially self-centering behavior; Self centering; Self-centering system; Genetic algorithms},
	correspondence_address = {X. Lei; Department of Civil and Environmental Engineering, The Hong Kong Polytechnic University, Kowloon, Hong Kong, China; email: leixiaoming@outlook.com},
	publisher = {Elsevier Ltd},
	issn = {23527102},
	language = {English},
	abbrev_source_title = {J. Build. Eng.},
	type = {Article},
