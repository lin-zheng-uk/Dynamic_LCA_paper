@article{ WOS:000640901400004,
Author = {Askarian, Saeed and Fakher, Ali},
Title = {Design of deep urban excavations using life cycle cost in comparison
   with acceptable risk and conventional method},
Journal = {TUNNELLING AND UNDERGROUND SPACE TECHNOLOGY},
Year = {2021},
Volume = {112},
Month = {JUN},
Abstract = {This paper aims to investigate risk based design of deep urban
   excavations, considering three case reports. Random Sets (RS)
   reliability analysis combined with finite element methods are deployed
   to consider the variability of ground parameters and to obtain the
   envelopes of probability of deformation for excavation walls. These
   envelopes are used to estimate the probability of deformation in
   surrounding buildings and infrastructures. Then, loss estimation is
   performed based on deformation probability. Finally, different design
   approaches including (i) Life Cycle Cost (LCC), (ii) acceptable risk and
   (iii) conventional method are compared together for three projects.
   Results show that conventional methods may not lead to the best suited
   design and the transition from conventional to risk based design in the
   context of deep excavations is an important necessity. The LCC method is
   evaluated in some real projects and it is shown that LCC is an efficient
   tool for risk based design of deep urban excavations.},
