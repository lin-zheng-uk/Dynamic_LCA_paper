Language = {English},
Affiliation = {Yi, MY (Corresponding Author), Korea Adv Inst Sci \& Technol, Grad Sch Data Sci, Dept Ind \& Syst Engn, Daejeon, South Korea.
   Ji, Sukwon, Korea Aerosp Res Inst, Infrastruct Integrated Support Off, Daejeon, South Korea.
   Lee, Bumho; Cho, Yesung; Yi, Mun Yong, Korea Adv Inst Sci \& Technol, Grad Sch Data Sci, Dept Ind \& Syst Engn, Daejeon, South Korea.},
DOI = {10.1016/j.jobe.2023.107028},
