Title = {Building Information Management as a Tool for Managing Knowledge
   throughout whole Building Life Cycle},
Booktitle = {3RD WORLD MULTIDISCIPLINARY CIVIL ENGINEERING, ARCHITECTURE, URBAN
   PLANNING SYMPOSIUM (WMCAUS 2018)},
Series = {IOP Conference Series-Materials Science and Engineering},
Year = {2019},
