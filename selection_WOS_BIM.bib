@article{ WOS:000336432800005,
Author = {Marzouk, Mohamed M. and Hisham, Mohamed and Al-Gahtani, Khalid},
Title = {Applications of bridge information modeling in bridges life cycle},
Journal = {SMART STRUCTURES AND SYSTEMS},
