@article{ WOS:001120732300001,
Author = {Kuai, Xi and Liu, Yu and Bi, Mingyan and Luo, Qinyao},
Title = {Deciphering Building Information Modeling Evolution: A Comprehensive
   Scientometric Analysis across Lifecycle Stages},
Journal = {BUILDINGS},
