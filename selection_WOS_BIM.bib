Title = {Research into the Building Information Model during the Whole Building
   Life-cycle},
Booktitle = {ADVANCES IN CIVIL ENGINEERING AND ARCHITECTURE INNOVATION, PTS 1-6},
Series = {Advanced Materials Research},
Year = {2012},
