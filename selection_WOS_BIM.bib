@article{ WOS:000482245500014,
Author = {Najjar, Mohammad and Figueiredo, Karoline and Hammad, Ahmed W. A. and
   Haddad, Assed},
Title = {Integrated optimization with building information modeling and life
   cycle assessment for generating energy efficient buildings},
Journal = {APPLIED ENERGY},
Year = {2019},
