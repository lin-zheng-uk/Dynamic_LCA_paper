@article{ WOS:000319258800002,
Author = {McGrath, Teresa and Nanukuttan, Sreejith and Owens, Kieran and Basheer,
   Muhammed and Keig, Peter},
Title = {Retrofit versus new-build house using life-cycle assessment},
