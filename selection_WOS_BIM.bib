Author = {Gardezi, Syed Shujaa Safdar and Shafiq, Nasir and Hassan, Ishtiaq and
   Arshid, M. Usman},
Title = {Life Cycle Carbon Footprint Assessments, Case Study of Malaysian Housing
   Sector},
