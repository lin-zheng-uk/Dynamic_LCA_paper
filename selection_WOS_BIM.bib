@article{ WOS:000546633900001,
Author = {Yao, Jian},
Title = {Building cooling energy uncertainty and life cycle economic performance:
   A stochastic air conditioning behavior model based comparative analysis},
Journal = {JOURNAL OF CLEANER PRODUCTION},
