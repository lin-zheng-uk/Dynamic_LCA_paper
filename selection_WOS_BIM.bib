@article{ WOS:000257960600009,
Author = {Junnila, Seppo},
Title = {Life cycle management of energy-consuming products in companies using
   IO-LCA},
Journal = {INTERNATIONAL JOURNAL OF LIFE CYCLE ASSESSMENT},
