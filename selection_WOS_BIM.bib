@inproceedings{ WOS:000566943700091,
Author = {Tchana, Yvan and Ducellier, Guillaume and Remy, Sebastien},
Editor = {Putnik, GD},
Title = {Designing a unique Digital Twin for linear infrastructures lifecycle
   management},
