@inproceedings{ WOS:000389248204012,
Author = {Zhang, Cheng and Chen, Jia and Sun, Xiao and Hammad, Amin},
Editor = {Tolk, A and Yilmaz, L and Diallo, SY and Ryzhov, IO},
Title = {LIFECYCLE EVALUATION OF BUILDING SUSTAINABILITY USING BIM AND RTLS},
Booktitle = {PROCEEDINGS OF THE 2014 WINTER SIMULATION CONFERENCE (WSC)},
Series = {Winter Simulation Conference Proceedings},
Year = {2014},
Pages = {3236-3247},
Note = {Winter Simulation Conference, Savannah, GA, DEC 07-10, 2014},
Abstract = {The purpose of this research is to provide a lifecycle building
   sustainability evaluation method to guide different stakeholders in how
   to apply sustainable practices and maintain the expected sustainability.
   Building Information Modeling (BIM) is selected to be a platform to
   integrate all the information to improve interoperability. Green
   standards are embedded in the BIM model and a rule-based system is
   developed to automatically evaluate the design and the building
   performance. Data are collected by using a Real-Time Location System
   (RTLS) and are used to update the BIM model. The as-built model is
   checked to see if it matches the sustainability aspects regarding the
   construction processes. During operation, energy consumption data are
   collected and analyzed. The performance of the building is checked to
   see if the designed features reach the sustainability goals. By
   integrating the BIM, RTLS, and other information, a prototype system of
   lifecycle sustainability evaluation is developed and tested.},
