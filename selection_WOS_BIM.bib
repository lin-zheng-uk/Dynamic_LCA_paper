@article{ WOS:000613777400027,
Author = {Kim, Byung Chul and Lee, Hyunoh and Mun, Duhwan and Han, Soonhung},
Title = {Lifecycle management of component catalogs based on a neutral model to
   support seamless integration with plant 3D design},
Journal = {JOURNAL OF COMPUTATIONAL DESIGN AND ENGINEERING},
