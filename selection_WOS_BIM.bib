   Jalaei, Farzad; Guest, Geoffrey; Gaur, Abhishek; Zhang, Jieying, Govt Canada, Civil Engn Infrastruct Construct, Natl Res Council Canada, Ottawa, ON, Canada.},
DOI = {10.1016/j.scs.2020.102294},
Article-Number = {102294},
ISSN = {2210-6707},
