@article{ WOS:000533656400007,
Author = {Hang Thu Thi Le and Likhitruangsilp, Veerasak and Yabuki, Nobuyoshi},
Title = {A BIM-Integrated Relational Database Management System for Evaluating
   Building Life-Cycle Costs},
Journal = {ENGINEERING JOURNAL-THAILAND},
