@article{ WOS:001276562200001,
Author = {Zelezna, Julie and Felicioni, Licia and Trubina, Nika and Vlasata,
   Barbora and Ruzicka, Jan and Veselka, Jakub},
Title = {Whole Life Carbon Assessment of Buildings: The Process to Define Czech
   National Benchmarks},
Journal = {BUILDINGS},
Year = {2024},
