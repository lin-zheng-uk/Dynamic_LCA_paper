@inproceedings{ WOS:000685059203028,
Author = {Nyvlt, Vladimir and Pruskova, Kristyna},
Editor = {Kliestik, T},
Title = {GLOBAL IMPACT ON LIFE CYCLE COST SAVINGS WITHIN LARGE TRANSPORTATION
   PROJECTS},
Booktitle = {GLOBALIZATION AND ITS SOCIO-ECONOMIC CONSEQUENCES, PTS I - VI},
Year = {2017},
Pages = {1796-1802},
Note = {17th International Scientific Conference on Globalization and Its
   Socio-Economic Consequences, Rajecke Teplice, SLOVAKIA, OCT 04-05, 2017},
Organization = {Univ Zilina, Fac Operat \& Econ \& Transport \& Commun, Dept Econ},
Abstract = {Managing large international projects namely within traffic and
   transport structures design processes need a special attention in terms
   of dealing with enormous amount of data and information. Principally, to
   achieve economically sustainable life cycle of any traffic and transport
   structure, employing of Building Information Management (BIM) tools
   should be explored and evaluated from the global perspective. BIM itself
   contains huge potential, how to increase effectiveness of every project
   in its all life cycle. It means from initial investment plan through
   project and building-up activities to long-term usage and property
   maintenance and finally demolition. Both important economical sets of
   tools, and managerial or technological, must be considered. Managerial
