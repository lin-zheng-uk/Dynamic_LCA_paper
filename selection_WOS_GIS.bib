Title = {A GIS-BASED APPROACH FOR THE ENERGY ANALYSIS AND LIFE CYCLE ASSESSMENT
   OF URBAN HOUSING STOCKS},
Booktitle = {EXPANDING BOUNDARIES: SYSTEMS THINKING IN THE BUILT ENVIRONMENT},
Year = {2016},
