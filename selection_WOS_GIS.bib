@article{ WOS:000760028900001,
Author = {Liu, Hua-Yueh and Lee, Hsi-Chieh},
Title = {The Carbon Inventory of the Reuse Phase's Life Cycle: The Example of the
   Reconstruction of a Zero-Carbon Campus on an Unused Military Camp},
