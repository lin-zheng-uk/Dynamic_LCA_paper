Title = {Completing the Missing Puzzle Piece of the Building Design Process:
   Modeling and Identifying Occupants' Satisfaction Level in Commercial
   Buildings},
Booktitle = {CONSTRUCTION RESEARCH CONGRESS 2016: OLD AND NEW CONSTRUCTION
   TECHNOLOGIES CONVERGE IN HISTORIC SAN JUAN},
Year = {2016},
