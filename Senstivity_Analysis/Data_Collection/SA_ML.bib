@article{ WOS:001224072600001,
Author = {Yang, Xian and Zhang, Chu and Li, Yijing and Tang, Chaolan and Liang,
   Peiqin},
Title = {Dynamic user profile construction and its application to smart
   product-service system design: A maternity-oriented case study},
