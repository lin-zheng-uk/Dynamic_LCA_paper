@inproceedings{ WOS:000393850200001,
Author = {Chung, Chi-Min and Chiang, Che-Ming},
Editor = {Deng, W},
Title = {A Prediction Model for the Raised Floor Coping with the Floor Impact
   Sound},
Booktitle = {2012 INTERNATIONAL CONFERENCE ON ECOLOGY, WASTE RECYCLING, AND
   ENVIRONMENT (ICEWE 2012)},
Series = {Advances in Biomedical Engineering},
Year = {2012},
Volume = {7},
Pages = {1-6},
Note = {International Conference on Ecology, Waste Recycling, and Environment
   (ICEWE 2012), Macao, PEOPLES R CHINA, APR 01-02, 2012},
Organization = {AASRI; APESRC; Univ Mauritius},
Abstract = {Due to the course of popular and commercial marketing, many buildings
   have not originally been designed by the users' individual diversity and
   requirements. As the existing building structure is difficult to be a
   change, in view of environmental protection and energy conservation
   issues, the proportion of reuse of the old buildings during their
   lifecycle is increasing, hence the flexible and variable floor would be
   the prevalence of interior decoration. In addition, upon the limitation
   of land resource and the high-rising trends of building development in
   Taiwan, and in case the building users or owners usually have not the
   ownership of their upper/underneath -story space, to solve the vertical
   noise interference would be the important issue for enhancement of
   acoustic environment quality. This study attempts to build a numerical
   analysis model to predict the effects of interior flexible and variable
   floor coping with the floor impact sound, by setting examples of the
   raised floor. The experiments and the assessments by use of light floor
   impact source in a full-scale chamber have proceeded. A Finite Element
   Method (FEM) model is built through a series of theory analysis and
   practical examination, and is demonstrated the effectiveness after
   comparison with the test data obtained from full-scale chamber
   experiments. On the basis of the effective FEM model, the different
   component factors of the raised floor upon the effects of floor impact
   sound insulation are examined by regression.},
Publisher = {INFORMATION ENGINEERING RESEARCH INST, USA},
Address = {100 CONTINENTAL DR, NEWARK, DE 19713 USA},
