@article{ WOS:000892647900110,
Author = {Kishore, Pranav and Karthigeyan, M. S. and Kini, M. Laxminarayan and
   Bhalla, Ankit and Didwania, Srijan},
Title = {Impact of the national building rating system on electricity demand of
   India},
Journal = {ENERGY REPORTS},
Year = {2022},
Volume = {8},
Number = {15},
Pages = {1021-1028},
Month = {NOV},
Note = {5th International Conference on Renewable Energy and Environment
   Engineering (REEE), Brest, FRANCE, AUG 24-26, 2022},
Abstract = {The building sector is one of the biggest consumers of electricity in
   India. Climate change and global warming, as one of the most serious
   risks to human societies, are inextricably linked to energy usage and
   Greenhouse gas emissions (GHG). With a population of nearly 1.38
   billion, India is the world's second largest country. The majority of
   Indian urbanization is involuted rather than evolved. Between 1991 and
   2011, the urban population expanded by 73 percent, from 217 million to
