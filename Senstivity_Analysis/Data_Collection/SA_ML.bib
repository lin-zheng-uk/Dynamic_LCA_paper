@inproceedings{ WOS:000865468500073,
Author = {Strachna, Olga and Asan, Onur},
Book-Group-Author = {IEEE COMP SOC},
Title = {Reengineering Clinical Decision Support Systems for Artificial
   Intelligence},
Booktitle = {2020 8TH IEEE INTERNATIONAL CONFERENCE ON HEALTHCARE INFORMATICS (ICHI
   2020)},
