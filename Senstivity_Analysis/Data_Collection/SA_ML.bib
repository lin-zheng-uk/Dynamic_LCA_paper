@article{ WOS:000305684200007,
Author = {Zhu, Qinghua and Zhao, Tielin and Geng, Yong},
Title = {Mediation Effects of Environmental Cooperation on the Relationship
   between Sustainable Design and Performance Improvement among Chinese
   Apartment Developers},
Journal = {SUSTAINABLE DEVELOPMENT},
Year = {2012},
Volume = {20},
Number = {3, SI},
Pages = {200-210},
Month = {MAY-JUN},
Abstract = {Due to increasing pressure for energy saving and emission reduction,
   Chinese apartment developers have initiated sustainable design
   practices. This paper develops a conceptual model which proposes that
   cooperation with construction stakeholders and customers is a mediator,
   necessary for apartment developers to realize environmental, economic
   and even competitive performance improvement through sustainable design
   practices. The paper examines the current practice situations of
   sustainable design, cooperation with construction stakeholders and
   customers as well as related performance improvement among Chinese
   apartment developers based on 92 questionnaires conducted by the
   authors. The mediation effects are further analysed through hierarchical
   regression analysis. Statistical results indicate that sustainable
   design should be further promoted by the Chinese governments and
   implemented by Chinese apartment developers. Policy implications are
   discussed to provide detailed suggestions on improving the cooperation
   with Chinese apartment developers and their construction
   stakeholders/customers. Such cooperation can help apartment developers
   to improve environmental, competitive and economic performance through
   sustainable design practices. Copyright (c) 2012 John Wiley \& Sons, Ltd
