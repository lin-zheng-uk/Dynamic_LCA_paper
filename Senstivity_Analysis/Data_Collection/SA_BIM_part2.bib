@article{ WOS:000391847800002,
Author = {Czarnecki, L. and Van Gemert, D.},
Title = {Scientific basis and rules of thumb in civil engineering: conflict or
   harmony?},
Journal = {BULLETIN OF THE POLISH ACADEMY OF SCIENCES-TECHNICAL SCIENCES},
