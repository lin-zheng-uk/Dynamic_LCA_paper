@inproceedings{ WOS:000320615005035,
Author = {Cheng, T. and Pandey, M. D.},
Editor = {Frangopol, DM and Sause, R and Kusko, CS},
Title = {Time-dependent reliability analysis of systems with repairable or
   non-repairable components},
Booktitle = {BRIDGE MAINTENANCE, SAFETY, MANAGEMENT AND LIFE-CYCLE OPTIMIZATION},
Year = {2010},
Pages = {3070-3077},
Note = {5th International Conference on Bridge Maintenance, Safety and
   Management (IABMAS), Lehigh Univ, Adv Technol Large Struct Syst (ATLSS)
   Engn Res Ctr, Philadelphia, PA, JUL 11-15, 2010},
Organization = {Digitexx Data Syst, Inc; U S Dept Transportat, Fed Highway Adm; Penn
   Infrastructure Technol Alliance (PITA); T Y Lin Int; Int Assoc Bridge
   Maintenance \& Safety (IABMAS)},
Abstract = {A generic methodology is presented for time-dependent reliability
   analysis of systems consisting of repairable and/or non-repairable
   components. The time to failure and repair time of components are
   considered as random variables, which are modeled by general
   (non-exponential).probability distributions. The time-dependent failure
   and repair rates are derived for each individual component of the system
   by solving a set of renewal equations. The component level information
   is used to compute the system failure rate and availability. The
   application of the proposed method is illustrated by the optimization of
   the life cycle cost of a structural system. It is noted that most of the
   literature on structural system reliability analysis focuses on systems
   with non-repairable components only. Thus the proposed approach is a
   significant generalization of the current approached for computing the
   life-cycle cost. The proposed method can be applied to a variety of
   infrastructure systems, such as bridges, buildings and power systems.},
