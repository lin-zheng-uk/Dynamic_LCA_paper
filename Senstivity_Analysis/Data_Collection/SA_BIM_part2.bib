@inproceedings{ WOS:000361831900072,
Author = {Chen, J. H. and Lu, S. C. and Wei, Z.},
Book-Group-Author = {Destech Publicat Inc},
Title = {Application of ECOTECT for the Evaluation of the Indoor Thermal
   Environment-A Case on the Teaching Building},
Booktitle = {INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE AND ENVIRONMENTAL
   ENGINEERING (CSEE 2015)},
Year = {2015},
Pages = {553-559},
Note = {International Conference on Computer Science and Environmental
   Engineering (CSEE), Beijing, PEOPLES R CHINA, MAY 17-18, 2015},
Abstract = {With the serious global warming, the building for long-term use showed
   huge energy consumption and waste. Therefore, the sustainable design
   strategies were viewed as main issues of energy saving by using suitable
   local design and longer life cycle. A teaching building of Chienkuo
   Technology University is selected as the study target. With the
   literature review, data collection and related theory discussion for the
   background information first, and the Ecotect software is applied for
   the evaluation of the indoor thermal environment by using Revit
   modelling. According to the Ecotect evaluation, the data accounting and
   analysis figures showed building conditions and indoor thermal
   environment simulation. Through the real field measurement and model
