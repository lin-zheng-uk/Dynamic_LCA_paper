@article{ WOS:000737602800001,
Author = {Simhachalam, Vyshali and Wang, Tong and Liu, Yan and Wamelink, Hans and
   Montenegro, Lorena and van Gorp, Geert},
Title = {Accelerating Building Energy Retrofitting with BIM-Enabled BREEAM-NL
   Assessment},
