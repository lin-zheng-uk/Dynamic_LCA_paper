@article{ WOS:001278821900001,
Author = {Meshref, Ahmed Nouh and Ibrahim, Abdelazim},
Title = {A dynamic approach for investigating design approaches to reducing
   construction waste in healthcare projects},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2024},
Volume = {95},
Month = {OCT 15},
Abstract = {Construction waste is a global problem that makes it hard for industry
   to reach its goals for sustainability. Over recent years, the Egyptian
   government has been investing millions of pounds in new and refurbished
   healthcare building projects. Various sustainability concerns have
   emerged throughout these advancements, including the generation of
   construction waste. This study investigates the effects of two
   techniques (design for deconstruction and prefabrication and
   modularization) as construction approaches to reduce construction waste
   in the design phase based on building information modeling (BIM)
   concepts for healthcare projects. Due to the complex and dynamic nature
   of construction projects, along with their interdependencies, system
   dynamic modeling (SDM) would be a suitable method to simulate the
   dynamic nature of such projects. The model incorporates every relevant
   factor that has an impact on the system when simulating the long-term
   effects of reducing construction waste in the construction life cycle of
   projects during the design phase for two techniques in six main aspects:
   time waste, cost waste, material waste, design changes, coordination
   among project stakeholders, and Energy efficiency and sustainability
   impacts. The system dynamics model was generated by processing this
   model using Any Logic software and a stock-flow diagram. Finally, a case
   study for the proposed model is used to illustrate the validation and
   implementation. The analysis revealed that Prefabrication and
   modularization have a crucial influence on managing the construction
   waste in the design phase. Utilizing the system dynamics model in this
   study enhances construction practices and facilitates informed
   decision-making within the field.},
