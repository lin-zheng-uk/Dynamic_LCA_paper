@article{ WOS:000477005500005,
Author = {Tucci, Grazia and Conti, Alessandro and Fiorini, Lidia and Corongiu,
   Manuela and Valdambrini, Noemi and Matta, Carlotta},
Title = {M-BIM: A NEW TOOL FOR THE GALLERIA DELL'ACCADEMIA DI FIRENZE},
