@inproceedings{ WOS:000222545600175,
Author = {Hinrichsen, S},
Editor = {Strasser, H and Kluth, K and Rausch, H and Bubb, H},
Title = {Holistic and multidimensional model for a service business as a frame of
   reference for organizational analysis, evaluation and design},
Booktitle = {QUALITY OF WORK AND PRODUCTS IN ENTERPRISES OF THE FUTURE},
Year = {2003},
Pages = {767-770},
Note = {Annual Spring Conference of the GfA/17th Annual Conference of the
   International-Society-for-Occupational-Ergonomics-and-Safety (ISOES),
   Munich, GERMANY, MAY 07-AUG 09, 2003},
Organization = {Fdn Gesell Arbeitswissensch; Int Soc Occupat Ergonom \& Safety},
Abstract = {The demands on factory planing as a industrial planing service are
   growing. The life cycles of real assets are shortening. Therefore the
   industrial enterprises as a costumer of factory planning services
   specify their demands on modifications and new building of factories at
   a later date. So working with uncertain information becomes more common.
   Furthermore the timeframe set by the costumer for the planing and
   building of factories shortens. Due to the changing demands on factory
   project work it gets increasingly harder to implement the required
   services in time and to the costs permitted. Thus it is more difficult
   to ensure the quality of the results. Against the background of this
   difficulty a model of service enterprises was developed. This model
   shows the essiantial fields of formation of the organizational
   development. It can also be used as a frame of reference for the
   analysis, evaluation and formation of organization at service
   enterprises. Furthermore the model shows the practitioner the main
   variables, which he can use to influence the quality of the results of a
   service enterprise.},
