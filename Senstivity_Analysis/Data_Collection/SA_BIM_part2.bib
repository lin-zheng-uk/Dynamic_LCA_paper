@article{ WOS:000649933800001,
Author = {Yitmen, Ibrahim and Alizadehsalehi, Sepehr and Akiner, Ilknur and
   Akiner, Muhammed Ernur},
Title = {An Adapted Model of Cognitive Digital Twins for Building Lifecycle
   Management},
