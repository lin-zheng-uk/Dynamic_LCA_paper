DOI = {10.1061/(ASCE)CP.1943-5487.0000277},
ISSN = {0887-3801},
EISSN = {1943-5487},
Keywords = {Building information models; Algorithms; Building information modeling
   (BIM); Industry Foundation Classes (IFC); Ontology; Partial model;
   Extraction algorithm},
Research-Areas = {Computer Science; Engineering},
Web-of-Science-Categories  = {Computer Science, Interdisciplinary Applications; Engineering, Civil},
