Title = {Improving Hazard Recognition in the Construction Industry: A Systematic
   Review of a Building Information Modeling Approach},
Booktitle = {CONSTRUCTION RESEARCH CONGRESS 2024: HEALTH AND SAFETY, WORKFORCE, AND
   EDUCATION},
