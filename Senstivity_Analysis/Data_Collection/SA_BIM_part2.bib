Title = {Perceptions of Augmented and Virtual Reality Technologies Adoption
   within the AEC Industry},
Booktitle = {CONSTRUCTION RESEARCH CONGRESS 2022: COMPUTER APPLICATIONS, AUTOMATION,
   AND DATA ANALYTICS},
Year = {2022},
