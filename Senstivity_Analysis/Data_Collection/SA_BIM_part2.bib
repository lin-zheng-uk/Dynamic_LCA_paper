@inproceedings{ WOS:000812323700030,
Author = {Groves, Lucy},
Book-Group-Author = {IEEE},
Title = {Quantifying the resilience potential of standalone PV and
   solar-plus-storage for commercial buildings nationwide},
Booktitle = {2021 IEEE PES INNOVATIVE SMART GRID TECHNOLOGIES - ASIA (ISGT ASIA)},
Year = {2021},
Note = {IEEE PES Innovative Smart Grid Technologies - Asia (ISGT Asia)
   Conference, Brisbane, AUSTRALIA, DEC 05-08, 2021},
Organization = {IEEE PES},
Abstract = {Extreme weather and cyber-attacks can cause power grid outages affecting
   thousands of buildings at a time, with long term economic consequences.
   Understanding the ability of commercial buildings to sustain their own
   critical energy load during grid outages is a necessary step in
   designing more energy resilient buildings. This study modeled commercial
   building sites nationwide and measured the expected resilience provided
   by distributed energy resources such as solar power and battery storage.
   Each of the 800 locations was configured with two building types, two
   outage scenarios, and solar and solar-plus-storage technology. Each
   configuration was then modeled using the REopt Lite design tool to
   determine the optimal sizing of energy resources to maintain the
   critical energy load during the outage at the lowest cost. Mean
   resilience hours and change in life cycle costs were summarized by
   climate zone. This survey indicated that solar power alone provided on
   average 2.1 hours of resilience, while solar-plus- storage provided on
   average 69.2 hours of resilience, and that sites with the greatest
   resilience and most savings were in climate zone.t-motst (Puerto Rico,
   Hawaii, Southern Florida). This information will aid building managers
   in planning sites with maximum energy resilience to prepare for future
   power grid outages.},
