@inproceedings{ WOS:000083193200029,
Author = {Pace, C and Gilda, G},
Editor = {Wang, KCP},
Title = {Cost and risk analyses in building systems},
Booktitle = {COMPUTING IN CIVIL ENGINEERING},
Year = {1998},
Pages = {235-246},
Note = {International Computing Congress Held in Conjunction with ASCE Annual
   Convention and Exposition on Information Technologies in Civil
   Engineering - Leading the World, BOSTON, MA, OCT 18-21, 1998},
Organization = {Amer Soc Civil Engineers, Tech Council Comp \& Informat Technol, Comm
   Coordinat Outside ASCE},
Abstract = {Cost analysis and value analysis techniques are frequently used to
   quantify and assess the economic implications for a particular project.
   These techniques have typically concentrated on utilizing life cycle and
   comparative cost procedures to determine either the lowest initial cost
   alternative or the highest investment return alternative While these
   techniques do provide a basis for making project cost decisions, they
   most often do not account for many of the parameters, which may affect
   the actual project value or cost.
   This paper uses the traditional value analysis techniques to review
   three exterior wall systems for a hypothetical retail building. These
   three systems include 8{''} concrete masonry, 2x6 wood studs, and 5
   1/2{''} concrete tiltup construction. Each system is analyzed to
   calculate the estimated initial building cost as well as the anticipated
   present worth of the building.
   The systems are also analyzed using simulation software to account for
   the potential variability of the parameters used in the cost and value
   models. The results of this technique provide probablistic information
   on the potential costs and values of the various wall system.
   By using this probabilistic approach, the expected cost and value ranges
   can more accurately reflect the actual life cycle costs of the system.
   With these results, the owner or design team can make strategic
   decisions based on analysis results that truly reflect the inherent
   risks and costs related to the project.},
