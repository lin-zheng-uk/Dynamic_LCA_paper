@inproceedings{ WOS:000467129900024,
Author = {Gavali, Hindavi R. and Ram, Shashi and Ralegaonkar, Rahul V.},
Editor = {Singh, UP and Babu, GLS},
Title = {Evaluation of Energy Efficient Sustainable Walling Material},
Booktitle = {URBANIZATION CHALLENGES IN EMERGING ECONOMIES: RESILIENCE AND
   SUSTAINABILITY OF INFRASTRUCTURE},
Year = {2018},
Pages = {227-233},
Note = {American-Society-of-Civil-Engineers (ASCE) India Conference on
   Urbanization Challenges in Emerging Economies - Resilience and
   Sustainability of Infrastructure, New Delhi, INDIA, DEC 12-14, 2017},
Organization = {Amer Soc Civil Engineers, India Sect; Indian Inst Technol Delhi; Inst
   Engineers},
Abstract = {Identification of alternative, sustainable materials, and technologies
   in traditional masonry work could decrease economic, environmental, and
   social impacts, such as energy usage and cost during the life-cycle of
   the building. In the present study, sustainable brick was developed
   using locally available industrial by-product (co-fired blended
   ash-CBA). Physico-mechanical, durability, and thermal conductivity tests
   were conducted on CBA bricks that fulfilled the requirements of Indian
   standard. Properties of the developed product were compared with the
   commercially available fly ash (FA) bricks. CBA bricks were found to be
   26\% lighter in weight with respect to FA bricks, which further caused
   the reduction in the dead load. In comparison with the FA bricks,
   embodied energy and thermal conductivity of the developed bricks was
   reduced by 23\% and 49.5\% respectively. The decreased thermal
   conductivity was helpful in reducing the operational energy demand of a
   building. The concept was briefed with a case study of a building over
   Nagpur (Maharashtra, India) that comes under the composite climatic zone
   as per SP 7: 2005. Building plan approved by the Indian standards and
   local government practice for single storey structure was modelled and
   analyzed in the simulation software (building information modelling
   tool) for the considered geographic location. With respect to FA bricks,
   CBA bricks reduced the peak-cooling load of a building by 8.3\%.
   Developed sustainable bricks were helpful in reducing the energy demand
   of the building in a cost effective manner. The study suggested that the
   application of novel sustainable building material with a computational
   approach strengthens the decision process.},
