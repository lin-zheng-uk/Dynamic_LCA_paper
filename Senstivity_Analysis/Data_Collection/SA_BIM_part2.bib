Author = {Kim, Young-Chan and Hong, Won-Hwa and Park, Jae-Woo and Cha, Gi-Wook},
Title = {An estimation framework for building information modeling (BIM)-based
   demolition waste by type},
Journal = {WASTE MANAGEMENT \& RESEARCH},
