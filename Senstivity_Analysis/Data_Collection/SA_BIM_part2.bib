@article{ WOS:000444996500064,
Author = {Seghier, Taki Eddine and Ahmad, Mohd Hamdan and Wah, Lim Yaik and
   Samuel, Williams Opeyemi},
Title = {Integration Models of Building Information Modelling and Green Building
   Rating Systems: A Review},
Journal = {ADVANCED SCIENCE LETTERS},
Year = {2018},
Volume = {24},
Number = {6},
Pages = {4121-4125},
Month = {JUN},
Note = {International Conference on Science, Engineering, Management and Social
   Sciences (ICSEMSS), Univ Teknologi Malaysia, Johor Bahru, MALAYSIA, OCT
   06-08, 2016},
Organization = {Univ Teknologi Malaysia, Int Student Soc; Int Students Ctr; Univ
   Teknologi Malaysia, Inst Sultan Iskandar},
Abstract = {The extensive impact of construction on the environment and the
   increasing demand for greener buildings have pushed forward the building
   industry to adopt new approaches for sustainable building design. In the
   light of this issue, many countries developed their own green rating
   system for building sustainability assessment and rating. However, the
   process of executing this task to benchmark building sustainability is
   based on fragmented and manual methods which is time-consuming,
   laborious and inefficient. Simultaneously, many research and real case
   studies have argued that the implementation of Building Information
   Modelling (BIM) in green building design process is advantageous
   throughout the building life cycle. One of the critical research topics
   in such implementation is the integration of BIM with green rating
   systems in order to assess building sustainability and facilitate green
   certification documents provision. Hence, the purpose of this paper is
   to review available BIM-based methodologies and proposed models made for
   green building assessment and certification, which have been developed
   to be integrated with several rating systems around the world such as
   LEED (R) and BREEAM (R). The results show that the integration of BIM
   with green building rating system is applicable. Nevertheless, still
   more focus on specific rating systems, mainly LEED (R). Thus, further
   research and development are required in this area to integrate other
   recent rating systems and enhance the level of automation by integrating
   green material databases and new BIM technologies such as visual
   programing and cloud-based databases.},
