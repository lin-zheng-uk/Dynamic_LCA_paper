@article{ WOS:001325821300001,
Author = {Bayat, Hossein and Ramezanianpour, Amir Mohammad},
Title = {Utilizing building information modeling (BIM) in the operation phase of
   civil infrastructure to analyze reinforcement corrosion induced by
   carbonation},
Journal = {INNOVATIVE INFRASTRUCTURE SOLUTIONS},
Year = {2024},
Volume = {9},
Number = {10},
Month = {OCT},
Abstract = {Increasing the service life and durability of structures is an important
   step toward sustainability. Today, the role of carbonation in the
   deterioration of reinforced concrete structures in urban areas is very
   prominent. Conversely, building information modeling (BIM) is an
   innovative tool used for sustainable design, facility management (FM),
   and infrastructure operation. It can provide visual representations of
   damage results that project decision-makers can use for future planning
   to extend the life cycle of structures. This research presents a
   BIM-based framework focusing on the carbonation process and the
   resulting corrosion in rebars. In this framework, by developing several
   plugins, the calculation results of the carbonation and corrosion of the
   rebars in an underground parking structure were transferred to the 3D
   model of the structure. Then, by adding a time dimension to the model,
   4D models and animations were created to visualize the process of
   concrete deterioration. Finally, the calculation results of this
   research and the 4D models can help create a maintenance schedule,
   analyze the time and cost of existing strategies to prevent concrete
   destruction, and provide an integrated plugin used to perform all
   calculations and create animations.},
