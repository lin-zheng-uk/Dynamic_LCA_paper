@inproceedings{ WOS:000189471800004,
Author = {van Iersel, TM},
Editor = {Anumba, CJ},
Title = {Innovative construction and design in cardboard},
Booktitle = {INNOVATIVE DEVELOPMENTS IN ARCHITECTURE, ENGINEERING AND CONTRUCTION},
Year = {2003},
Pages = {37-46},
Note = {2nd International Conference on Innovative Developments in Architecture,
   Engineering and Construction, Loughborough, ENGLAND, JUN 25-27, 2003},
Organization = {Ctr Innovat Construct Engn; Dept Civil \& Bldg Engn; Loughborough Univ;
   Loughborough LE113TU; Inst Civil Engn; Int Council Res\& Innovat Bldg \&
   Construct; Royal Inst British Architects},
Abstract = {The growing worldwide population has an increasing energy use absorption
   and natural materials resources exhaustion. At the same time the growing
   society demands more and more buildings, but the changing composition in
   time cause altering desires. This change is often denied during design
   and construction phase, resulting in large streams of waste. Use of
   lightweight and recycling materials (as cardboard) in construction, can
   improve both building environment effects and flexibility.
   Understanding change by looking at the various life spans of building
   (elements) will give insight and possibilities to reduce environmental
   impact. In this paper a Service Life Planning model will be discussed,
   focussing on the scenario of balanced energy use. The recycling aspects,
   technical performance range of various types of cardboard present at the
   industry together with the low-costs aspects show potential for building
   (elements) for short life cycle use. A hypothesis is stated wherein
   temporary buildings out of cardboard to give answer to ecological
   desires and altering technical demands changing in time.
   This paper will explore the technical and economical potential of
   cardboard, focussing on the various levels of barriers and possibilities
   in the cardboard industry, which hamper the introduction of cardboard in
   building industry. Accordingly, research topics that could improve
   efficiency of cardboard as sustainable building material will be
   accentuated.},
