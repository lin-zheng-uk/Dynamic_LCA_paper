Title = {Applying System Science and System Thinking Techniques to BIM Management},
Booktitle = {2017 10TH INTERNATIONAL CONFERENCE ON DEVELOPMENTS IN ESYSTEMS
   ENGINEERING (DESE 2017)},
Series = {International Conference on Developments in eSystems Engineering},
Year = {2017},
