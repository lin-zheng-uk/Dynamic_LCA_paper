Title = {Toward a BIM-based procedure for the evaluation of a risk prioritization
   class of bridge structures},
Booktitle = {EUROPEAN ASSOCIATION ON QUALITY CONTROL OF BRIDGES AND STRUCTURES,
   EUROSTRUCT 2023, VOL 6, ISS 5},
