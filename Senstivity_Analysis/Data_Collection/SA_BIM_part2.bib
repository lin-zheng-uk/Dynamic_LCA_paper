@article{ WOS:000483623500013,
Author = {Jin, Ruoyu and Zou, Yang and Gidado, Kassim and Ashton, Phillip and
   Painting, Noel},
Title = {Scientometric analysis of BIM-based research in construction engineering
   and management},
