@article{ WOS:000892768000001,
Author = {Godager, Bjoern and Mohn, Knud and Merschbrock, Christoph and Klakegg,
   Ole Jonny and Huang, Lizhen},
Title = {TOWARDS AN IMPROVED FRAMEWORK FOR ENTERPRISE BIM: THE ROLE OF ISO 19650},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY IN CONSTRUCTION},
Year = {2022},
Volume = {27},
Pages = {1075-1103},
Abstract = {Enterprise BIM aims to optimise business management and asset management
   throughout the life -cycle of buildings and infrastructures. This
   follow-up study of the Enterprise BIM concept analyses the potential for
   international standardisation, mainly focusing on the contribution of
   the ISO 19650 standard series to further development. The study explores
   how key ISO 19650 concepts related to different types of information
   requirements, levels of information need, and common data environments
   can help strengthen Enterprise BIM. As such, the paper presents an
   in-depth analysis of how the ISO 19650 series and several adjacent
   standards can provide a further means of facilitating information
   management within the Enterprise BIM concept. The study uses a
   customised Zachman framework by adding the necessary perspectives from
   ISO 19650 and Enterprise BIM. The paper adds increased knowledge related
   to understanding the standard series parts 1, 2 and 3 and showing how
   they can be implemented in an enterprise structure. Based on the ISO
   19650 framework, the study emphasises the importance of linking the
   information requirements to the various information needs of multiple
   end-users. At the same time, the paper provides new knowledge by showing
   that the information flow in the operational phase is far more nuanced
   than in the delivery phase, suggesting that information management must
   be considered in a completely different light from what previous
   research shows.},
Publisher = {INT COUNCIL RESEARCH \& INNOVATION BUILDING \& CONSTRUCTION},
Address = {KRUISPLEIN 25 G, PO BOX 1837, ROTTERDAM, 3000 BV, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Godager, B (Corresponding Author), Norwegian Univ Sci \& Technol, Dept Mfg \& Civil Engn, Trondheim, Norway.
   Godager, Bjoern; Mohn, Knud; Merschbrock, Christoph; Huang, Lizhen, Norwegian Univ Sci \& Technol, Dept Mfg \& Civil Engn, Trondheim, Norway.
   Klakegg, Ole Jonny, Norwegian Univ Sci \& Technol, Dept Civil \& Environm Engn, Trondheim, Norway.},
DOI = {10.36680/j.itcon.2022.053},
ISSN = {1874-4753},
Keywords = {Asset information model (AIM); Asset management (AM); Building
   information modelling (BIM); Common data environment (CDE); Enterprise
