@article{ WOS:001311152600018,
Author = {Werner, Jeremy and Esser, Kelli and Arndt, Craig and Radocaj, Trisha and
   Anyanhun, Awele and Wolodkin, Daniel and Kerr, Geoffrey and Freeman,
   Laura},
Title = {Integrated Decision Support Key: Advancing Acquisition Decisions with
   Data Models and Tools},
Journal = {NAVAL ENGINEERS JOURNAL},
Year = {2024},
Volume = {136},
Number = {1-2},
Pages = {121-133},
Month = {MAR 1},
Abstract = {The fundamental purpose of test and evaluation of a Department of
   Defense (DoD) acquisition program is to generate data that enables
   accurate understanding and knowledge about a system's operational
   performance in support of key acquisition or program decisions. The
   Integrated Decision Support Key (IDSK) is defined in DoD policy to
   effectively enable this purpose by linking key acquisition or program
   decisions to the four high-level operational metrics-effectiveness,
   suitability, survivability, and lethality-and test data or modeling and
   simulation results supporting them. This connection between decisions
   and the data supporting them is a dynamic process that captures the
   evolution of our understanding of the operational performance of the
   system as it matures across its life cycle, becoming more definitive as
   more data become available. In its optimized form, the IDSK is a
   dynamic, data-driven decision support tool. The utility of the IDSK can
   be maximized with adequately established connections to information
   spanning multiple phases of test and environments. Despite this promise,
   implementing the IDSK has proven a challenge, primarily due to the lack
   of well-defined but flexible IDSK data models with suggested information
   content and accompanying tools for building them in such a way that does
   not require specialized technical expertise. We have largely overcome
   this challenge and present four IDSK exemplars in this article to
   demonstrate how this decision support key can be developed based on
   well-defined but flexible data models using a variety of tools. We
   provide links to interactive versions of these items, including an
   easy-to-use prototype can start using now to explore and build digital
   IDSKs.},
