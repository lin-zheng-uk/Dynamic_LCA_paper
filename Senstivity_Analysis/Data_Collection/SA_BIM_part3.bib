@article{ WOS:000427544000119,
Author = {Cheung, Weng-Fong and Lin, Tzu-Hsuan and Lin, Yu-Cheng},
Title = {A Real-Time Construction Safety Monitoring System for Hazardous Gas
   Integrating Wireless Sensor Network and Building Information Modeling
   Technologies},
