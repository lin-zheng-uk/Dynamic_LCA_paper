@article{ WOS:000482176800190,
Author = {Natarajan, Anupama and Sethumadhavan, Anjali and Krishnan, Uma Maheswari},
Title = {Toward Building the Neuromuscular Junction: In Vitro Models To Study
   Synaptogenesis and Neurodegeneration},
