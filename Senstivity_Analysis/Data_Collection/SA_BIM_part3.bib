@article{ WOS:000557144200001,
Author = {Badran, Deena and AlZubaidi, Radhi and Venkatachalam, Senthilkumar},
Title = {BIM based risk management for design bid build (DBB) design process in
   the United Arab Emirates: a conceptual framework},
Journal = {INTERNATIONAL JOURNAL OF SYSTEM ASSURANCE ENGINEERING AND MANAGEMENT},
Year = {2020},
