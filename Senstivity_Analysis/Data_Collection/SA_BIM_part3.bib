@article{ WOS:000997241700001,
Author = {Demirdogen, Gokhan and Isik, Zeynep and Arayici, Yusuf},
Title = {BIM-based big data analytic system for healthcare facility management},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2023},
Volume = {64},
Month = {APR 1},
Abstract = {Facility management (FM) is the costliest stage in the building
   lifecycle of healthcare buildings. Building Information Modeling (BIM)
   has potential solution to enable information management for healthcare
   FM. However, BIM is not used at FM due to its static nature, while
   healthcare FM requires dynamic information handling. The FM systems in
   practice and literature require either implementation of WebGL-HTML5
   applications that necessitates coding knowledge for updates or data
   analysis. Therefore, this paper aims to develop a healthcare FM system
   based on Big Data Analytic (BDA), BIM and Not only SQL (NoSQL) database
   to enable FM information query and FM-KPI (Key Performance Indicator)
   visualization. The proposed system is developed through Design Science
   Research methodology. Findings show that the system is effective and
   practical to perform FM data retrival and analysis by practioners in
   healthcare facilities management effec-tively.},
