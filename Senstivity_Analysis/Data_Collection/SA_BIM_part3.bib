@article{ WOS:000960623400001,
Author = {Srivastava, Deepanshu and Komma, Venkateswara Rao},
Title = {STEP-NC AP238-an excellent paradigm for smart manufacturing},
Journal = {INTERNATIONAL JOURNAL OF INTERACTIVE DESIGN AND MANUFACTURING - IJIDEM},
Year = {2023},
Volume = {17},
Number = {4},
Pages = {1475-1487},
Month = {AUG},
Abstract = {Information exchange plays a crucial role in digital manufacturing.
   Newer paradigm such as Industry 4.0 utilizes data transfer through
   sophisticated technologies of sensors, internet, intelligent system, IoT
   and other related technologies across enterprise. Above concepts tends
   to create manufacturing sector into ``Smart Industries{''}. Exchange of
   product model information without human intervention helps in automation
   and integration of different sections of the manufacturing organization.
   STEP (STandard for Exchange of Product model data) used EXPRESS
   language, represents information of product model data in object
   oriented structure. STEP contains different Application Protocols (APs),
   which are suitable for different domains of product life cycle. AP203
   represents geometrical and topological information of product. AP224
   represents feature information of product in a well-defined structure.
   STEP-NC codes have data of manufacturing features as well as
   manufacturing process; it evolved as a superior format for computer
   control of machine tools as compared to G-M codes. STEP-NC was
   standardised in two forms, namely, ISO 14,649 and ISO 10303-238 (AP238).
   AP238 has rich set of information, which can be used across product
   lifecycle, as it uses STEP generic resources for building a particular
   model. STEP based methods and their application areas, seem to be an
   excellent conveyor of smart manufacturing or Industry 4.0, which would
   help in complete integration of CAD/CAPP/CAM. In this article, a
   detailed review of works related different STEP methods are presented
   along with detailed study of past research using different STEP formats.
   From this review study, it can be concluded that STEP AP238 provides an
   effective, efficient and sustainable format for seamless integration of
   various activities related to design and manufacturing processes
   involved in scenario of Industry 4.0 or Smart Manufacturing.},
