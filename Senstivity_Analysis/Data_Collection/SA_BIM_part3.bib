@inproceedings{ WOS:000380478000016,
Author = {Dubois, Eric and Mauger, Cyril},
Book-Group-Author = {IEEE},
Title = {Extended Architectural Models: First Steps towards the Reconciliation of
   the `Soft' and the `Hard' Parts of an Enterprise},
Booktitle = {PROCEEDINGS OF THE 2015 IEEE 19TH INTERNATIONAL ENTERPRISE DISTRIBUTED
   OBJECT COMPUTING CONFERENCE WORKSHOPS AND DEMONSTRATIONS (EDOCW 2015)},
Series = {IEEE International Enterprise Distributed Object Computing Conference
   Workshops-EDOCW},
Year = {2015},
Pages = {125-129},
Note = {IEEE 19th International Enterprise Distributed Object Computing (EDOCW),
   Adelaide, AUSTRALIA, SEP 21-25, 2015},
Organization = {CPS; IEEE Comp Soc; Univ S Australia; Adv Comp Res ctr; Serv Sci Soc
   Australia; IEEE; IEEE Commun Soc},
Abstract = {Enterprise Architecture is now widely recognized as a central discipline
   for managing and governing the evolution of an organization, in
   particular from an Information System perspective. EA models are
   relevant techniques with associated tools for mastering and reasoning
   about the complexity of organizations.
   In the construction domain, architectural models are used for a long
   time and support building architects for managing complex constructions
   projects and their associated building lifecycles.
   In this position paper, we suggest how the two types of model can nicely
   complement. In the specific context of ``smart buildings{''} where the
   `hard' construction component is more and more intertwined with a `soft'
   IS/IT component, we discuss and illustrate some needs regarding the
   benefits associated to the reconciliation of the two types of models.
