Keywords = {Building information modelling (BIM); Facility management; Asset data
   management; Lifecycle data exchange; Asset information requirements
   (AIR); COBie Process; BIM; Digital engineering},
Keywords-Plus = {FACILITY MANAGEMENT; INFORMATION; REQUIREMENTS},
Research-Areas = {Business \& Economics},
