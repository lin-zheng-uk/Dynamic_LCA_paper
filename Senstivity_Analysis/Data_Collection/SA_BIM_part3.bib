@article{ WOS:000365649500019,
Author = {Rey, E. and Aguayo, F. and Peralta, Ma E. and Lama, J. R. and Avila, Ma
   J.},
Title = {Integrated scalar and vector method for fire risk assessment of
   lifecycle construction},
Journal = {INFORMES DE LA CONSTRUCCION},
Year = {2015},
Volume = {67},
Number = {539},
Month = {JUL-SEP},
Abstract = {Fire is one the main risks in the construction sector. Consequences as
   material losses, stoppage and possible personal damages cause the fire
   risk assessment and knowledge of building behaviour in fire situation
   are essential to prevent and minimize the consequences. Currently there
   are sets of conventional scalar methods that quantify the risk level and
