@article{ WOS:000910650400001,
Author = {Gu, Jinlei and Wang, Jiacun and Guo, Xiwang and Liu, Guanjun and Qin,
   Shujin and Bi, Zhiliang},
Title = {A Metaverse-Based Teaching Building Evacuation Training System With Deep
   Reinforcement Learning},
