@inproceedings{ WOS:000262502700001,
Author = {Angelica, Ospina-Alvarado and Daniel, Castro-Lacouture and Kathy, Roper},
Editor = {Wang, YW and Xia, KW and Shen, QP},
Title = {Emerging Framework for A/E/C/FM Integration},
Booktitle = {PROCEEDINGS OF 2008 INTERNATIONAL CONFERENCE ON CONSTRUCTION \& REAL
   ESTATE MANAGEMENT, VOLS 1 AND 2},
Year = {2008},
Pages = {1-5},
Note = {International Conference on Construction and Real Estate Management,
   Toronto, CANADA, OCT 04-05, 2008},
Organization = {Harbin Inst Technol; Hong Kong Polytech Univ; Univ Toronto; Univ
   Florida; Purdue Univ; Univ Salford; Univ W England; Florida Int Univ;
   Natl Univ Singapore},
Abstract = {Presently, the. construction industry is facing several challenges:
   globalization of the marketplace; increased complexity; larger
   investments; diversity of disciplines involved; widespread stakeholders;
   tight schedules with limited resources; stringent quality standards; and
   changes of project scope. The industry is responding to these challenges
   by understanding the need to rapidly evolve toward an integrated project
   lifecycle. To support the integration of the construction process,
   several methods are emerging. These methods combine isolated theories,
   tools and philosophies that have impacted the construction industry for
   the past decade, such as Building Information Modeling (BIM), Leadership
   in Energy and Environmental Design (LEED), lean construction, etc.
   However, it is difficult to quantify the extent to which those combined
   methods are contributing to process integration. This paper develops a
   framework to define A/E/C/FM integration from a collaboration
   perspective, and to measure the contribution of those combined methods
   towards integration. To develop this framework, the concept of
   integration is defined based on literature findings and expert opinion;
   subsequently, a process model of the construction lifecycle is created
   based on the integrated project delivery approach, defining inputs and
   outputs, stakeholders involved and their interactions, and indicators
   that measure the success/failure of such interactions. The process map
   will be validated through expert opinion. Indicators from the process
   map will be used to per-form a survey to a sample of stakeholders to
   assign weights of importance.},
