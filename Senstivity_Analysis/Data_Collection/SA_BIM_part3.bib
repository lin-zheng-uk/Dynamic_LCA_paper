Title = {Introduction to (teaching/learning about) digital libraries},
Booktitle = {PROCEEDINGS OF THE 5TH ACM/IEEE JOINT CONFERENCE ON DIGITAL LIBRARIES,
   PROCEEDINGS},
Series = {ACM-IEEE Joint Conference on Digital Libraries JCDL},
