@article{ WOS:001298298100001,
Author = {Anthony, Reynold and Anandh, K. S. and Prasanna, K. and Sawhney, Anil},
Title = {An exploratory study on the application of digital twins in the Indian
   construction industry},
Journal = {INTERNATIONAL JOURNAL OF CONSTRUCTION MANAGEMENT},
Year = {2024},
Month = {2024 AUG 13},
Abstract = {Digitalization, enhancing operational performance and promoting
   data-driven practices, is gaining prominence globally. However, its
   adoption in the construction industry, particularly in developing
   countries like India, is still in its infancy. This study explores the
   role and impact of digital twins, a key component of digital
   transformation, in the Indian construction sector. A cross-sectional
   study was conducted using qualitative and quantitative methods,
   collecting data from various roles within organizations primarily
   engaged in building information modeling. The aim is to understand the
   feasibility, acceptability, and adaptability of digital twins in the
   construction sector. Results indicate that identified factors positively
   influence these constructs. Despite a preference for human intervention,
   the study predicts significant traction for automated methods in the
   Indian construction sector. The survey factors strongly influence the
   feasibility, acceptability, and adaptability of digital twins in Indian
   construction organizations, demonstrating the potential to adapt and
   incorporate innovative technologies throughout the project lifecycle.
   Despite risks and challenges, Indian construction firms can adopt
   digital twins in projects.},
