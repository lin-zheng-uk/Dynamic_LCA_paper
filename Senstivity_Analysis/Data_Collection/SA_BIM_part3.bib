@inproceedings{ WOS:000227836700032,
Author = {Strassner, J},
Editor = {Chu, HW and Aguilar, J and Ferrer, J and Syau, YR and Cheng, CB},
Title = {Modeling and managing the lifecycle of telecom systems},
