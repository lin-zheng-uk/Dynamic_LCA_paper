@article{ WOS:001016995300001,
Author = {Liu, Zhen and Gong, Shiqi and Tan, Zhiya and Demian, Peter},
Title = {Immersive Technologies-Driven Building Information Modeling (BIM) in the
   Context of Metaverse},
