Title = {Marketing in the XXI Century: from Finding a Customer for Business to
   Finding ``Necessary{''} Business for the Customer},
Booktitle = {SOCIETY, INTEGRATION, EDUCATION, VOL VI: ECONOMICS, INNOVATIVE BUSINESS
   AND PUBLIC ADMINISTRATION - FINANCE, ACCOUNTING AND TAX ADMINISTRATION},
Series = {Sabiedriba Integracija Izglitiba-Society Integration Education},
Year = {2019},
