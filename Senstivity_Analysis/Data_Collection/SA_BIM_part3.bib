Title = {PARAMETRIC HERITAGE. THE DIGITAL RECONSTRUCTION OF PL NERVI'S PALAZZO
   DEL LAVORO IN THE 3DMODELLING AGE},
Booktitle = {27TH CIPA INTERNATIONAL SYMPOSIUM: DOCUMENTING THE PAST FOR A BETTER
   FUTURE},
Series = {International Archives of the Photogrammetry, Remote Sensing and Spatial
   Information Sciences},
