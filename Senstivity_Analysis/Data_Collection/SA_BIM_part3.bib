@article{ WOS:000258779400011,
Author = {Fu Jihua and Meng Hao and Wang Zhongyu},
Title = {Full-lifecycle grey accuracy modeling for the dynamic measurement system},
Journal = {JOURNAL OF GREY SYSTEM},
Year = {2007},
Volume = {19},
Number = {3},
Pages = {283-292},
Abstract = {A grey accuracy model for the dynamic measurement system in
   full-lifecycle is put forward to analyse the law of the accuracy motion,
   forecasts and diagnose the system's accuracy. On the basis of the
   information coverage principle; the uncertainty and the absolute values
   of the measuring errors are selected to build the rolling grey model. By
   building two rolling grey models with different data lengths, the
   estimated values for the accuracy and the general trend are obtained. By
   the difference of the two estimated values. the random fluctuation is
   obtained. Through the sample and simulation analysis, the general trend
   and the random fluctuation are obtained at the same time by the two
   rolling grey models, and the accuracy motion of the dynamic measurement
   system can be forecasted. For non-statistical and generating modeling.
   the grey accuracy modeling method can solve the problems of less data,
   lack of information and uncertainty in modeling. The method can
   eliminate the needs for large sample and canonical distribution, and it
   is simple to process.},
