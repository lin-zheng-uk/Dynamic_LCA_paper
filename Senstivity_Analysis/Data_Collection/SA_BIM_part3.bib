Title = {Investigating the Potential of Delivering Employer Information
   Requirements in BIM Enabled Construction Projects in Qatar},
Booktitle = {PRODUCT LIFECYCLE MANAGEMENT IN THE ERA OF INTERNET OF THINGS, PLM 2015},
Series = {IFIP Advances in Information and Communication Technology},
Year = {2016},
