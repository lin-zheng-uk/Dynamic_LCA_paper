Editor = {Bernhardt, BR and Hinds, LH and Strauch, KP},
Title = {What Are Subject Liaisons When ``Collections{''} and ``Subjects{''}
   Don't Matter?},
Booktitle = {ROLL WITH THE TIMES OR THE TIMES ROLL OVER YOU},
