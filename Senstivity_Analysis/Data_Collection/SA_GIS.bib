@inproceedings{ WOS:000296180800045,
Author = {Yuan, Zhao and Shen, Geoffrey Q. P.},
Editor = {Wang, Y and Yang, J and Shen, GQP and Wong, J},
Title = {Using IFC Standard to Integrate BIM Models and GIS},
Booktitle = {PROCEEDINGS OF 2010 INTERNATIONAL CONFERENCE ON CONSTRUCTION AND REAL
   ESTATE MANAGEMENT, VOLS 1-3},
Year = {2010},
Pages = {224-229},
Note = {2010 International Conference on Construction and Real Estate
   Management, Brisbane, AUSTRALIA, DEC 01-03, 2010},
Organization = {Queensland Univ Technol, Australia; Harbin Inst Technol; Hong Kong
   Polytechn Univ, P R China; Univ Florida; Univ Salford, UK; Univ W
   England, UK; Florida Int Univ; Natl Univ Singapore, Singapore},
Abstract = {City informationization is the future trend of urban planning and city
   management, in which the concept of digital cities in recent years been
   repeatedly mentioned. 3D models and geographic information system (GIS)
   are the most important parts to describe a digital city in multi-rate,
   multi-scale and space-time dimensions, namely the use of information
   technology to simulate city's past, present and future on digital and
   virtual implementation. However, 3D models without the buildings'
   internal information cannot provide sufficient information support to a
   digital city. Building Information Modeling (BIM) has complete
   information of a building's whole life cycle. Therefore, integration of
   BIM and GIS is the future trend of urban management.
   This paper reviews the features of BIM and GIS, discusses their
   advantages and weaknesses in this context, and then proposes the concept
   of integration of BIM models and GIS. However, there will be many
   problems to face in order to combine BIM and GIS. An Industry Foundation
   Classes (IFC) standard has been proposed to solve these problems. This
   paper also reviews the IFC standard and its application in conversion of
   BIM models. By comparison of three different IFC formats which were
   converted from a BIM model established by Autodesk Revit Architecture,
   some significant losses of details have been found in the converting
   process. Meanwhile, how to integrate BIM models and GIS has been
   discussed.},
