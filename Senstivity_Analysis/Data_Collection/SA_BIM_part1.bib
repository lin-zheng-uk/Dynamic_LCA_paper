@article{ WOS:001256947300001,
Author = {Celoza, Amelia},
Title = {Information management for digital asset development and delivery},
Journal = {AUTOMATION IN CONSTRUCTION},
Year = {2024},
Volume = {165},
Month = {SEP},
Abstract = {The architecture, engineering, construction, and facility management
   (AEC/FM) industry is undergoing a digital transformation, and digital
   assets are emerging to support the development and delivery of physical
   assets. While these digital assets can create value throughout a
   project's life cycle and the form of digital assets has been previously
   studied, there is a dearth of research focused on how team processes
   must change to optimize the implementation of these digital tools.
   Therefore, the research objective was to characterize processes,
   enablers, and barriers related to digital asset development in AEC/FM
   projects. This paper implemented a systematic literature review of 73
   journal articles published through December 31, 2023. This work
   identified 13 processes, 19 enablers, and 20 barriers to developing and
   delivering digital assets to support physical assets throughout their
   life cycle. This work supports future research to enhance the AEC/FM
   industry's ability to leverage technology to support project delivery
   and performance.},
