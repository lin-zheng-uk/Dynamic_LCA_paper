@article{ WOS:001117003700001,
Author = {Tushar, Quddus and Sun, Weihan and Zhang, Guomin and Navaratnam,
   Satheeskumar and Hou, Lei and Giustozzi, Filippo},
Title = {Evolution in impacts assessment for managing and recycling of waste: A
   scientometric analysis},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2023},
Volume = {430},
Month = {DEC 10},
Abstract = {This study aims to conduct a scientometric analysis to identify emerging
   trends in waste management strategies over the last three decades
   (1994-2003, 2004-2013, and 2014-2023). Integrated application of
   bibliometric analysis (VOSviewer) and Science Mapping Analysis Tool
   (SciMAT) facilitates the tracing pattern of potential research themes,
   influential authors, significant outcomes, and relevant contributing
   factors over this specified period. Four prominent themes of the
   strategic diagram: (1) Management Systems, (2) Building Information
   Modeling (BIM), (3) Polymer, and (4) Eco-Neighbourhood have been
   revealed for further exploration and improvement. Clustering algorithms
   categorize the principle theme into further sub-components of resource
   recycling, project or building life cycle, energy analysis,
   environmental cost, waste plastic, geopolymer, and others to provide a
   nuanced understanding of interconnected themes. A pragmatic application
   of this analysis minimizes waste generation, optimizes resource
   recovery, and mitigates waste disposal's environmental impacts and
   health consequences. This review article's evidence-based framework
   assesses research dynamics, highlights potential sectors for
   exploration, and encourages innovation and collaboration among
   policymakers and the scientific community to facilitate informed and
   sustainable decision-making. However, valuable insights from alternative
   sources such as technical reports and non-English scholarly articles
   have not been considered for achieving practical applications.},
