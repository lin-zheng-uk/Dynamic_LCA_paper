@article{ WOS:001099893400001,
Author = {Li, Clyde Zhengdao and Li, Shuo and Ya, Yingyi and Tam, Vivian W. Y.},
Title = {Digital inspection techniques of modular integrated construction},
Journal = {HELIYON},
Year = {2023},
Volume = {9},
Number = {11},
Month = {NOV},
Abstract = {As a new construction form, modular integrated construction (MiC) can
   effectively improve the construction quality and productivity,
   especially for the construction of high-density and highrise buildings.
   However, the current MiC quality inspection relies on manual inspection,
   which is inefficient and unreliable. Systematic research on digital
   inspection techniques (DITs) is frag-mented and unable to fully realize
   the potential of the MiC industry. This study aims to explore the
   current state of DIT applications in MiC and to summarize the knowledge
   in the field through an analysis of 248 relevant literatures.
   Accordingly, this study combines bibliometric analysis, and a system
   engineering evaluation approach based on 3D structures (time, knowledge,
   and logic) to provide an overview of the current state of DIT
   development. The overview includes the application of DITs from a whole
   life cycle perspective, the DIT knowledge structure, specific DIT
   applications, as well as current challenges and future prospects.},
