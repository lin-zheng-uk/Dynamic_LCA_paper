@article{ WOS:001318009300001,
Author = {Lekeufack, Beco Chenadaire and Fu, Bo and Samuel, Mabor Achol and Alam,
   Shahin},
Title = {Seismic resilience assessment of coastal carbon and weathering steel
   moment frame considering uniform atmospheric corrosion effects},
Journal = {STRUCTURES},
Year = {2024},
Volume = {69},
Month = {NOV},
Abstract = {Carbon steel (CS) has been extensively used in the construction of steel
   moment frame buildings in coastal, industrial, and rural regions.
   However, its performance under multi-hazards, such as the combined
   atmospheric corrosion and earthquake loads, has been largely overlooked.
   In contrast, weathering steel (WS) has received less attention as an
   alternative material to increase the life cycle resilience due to
   corrosion. This work aims to fill this research void by investigating
   the seismic resilience of building structures constructed with CS and WS
   materials. To achieve this goal, a probabilistic seismic resilience
   assessment of three different models called non-corroded steel frame
   (NCSF), corroded carbon steel frame (CCSF), and corroded weathering
   steel frame (CWSF) is performed through the incremental dynamic analysis
   (IDA). The findings indicate a significant reduction in the seismic
   resilience of CCSF with an increase of the life cycle cost compared to
   NCSF. In contrast, CWSF experienced an increase in resilience and a
   decrease in the life cycle cost compared to CCSF. These results
   underscore the critical need for regular maintenance and corrosion
   mitigation strategies in CS structures, particularly in areas prone to
   high humidity and seismic activity. This study contributes to a broader
   understanding of the interplay between the environmental degradation and
   structural safety, providing a valuable, insightful information for the
   design and maintenance of safer seismic-resistant structures.},
