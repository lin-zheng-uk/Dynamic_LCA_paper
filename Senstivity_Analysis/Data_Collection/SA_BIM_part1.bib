@inproceedings{ WOS:000541116600016,
Author = {Xu, Jinying and Lu, Weisheng},
Editor = {Wang, C and Harper, C and Lee, Y and Harris, R and Berryman, C},
Title = {Smart Construction from Head to Toe: A Closed-Loop Lifecycle Management
   System Based on IoT},
Booktitle = {CONSTRUCTION RESEARCH CONGRESS 2018: CONSTRUCTION INFORMATION TECHNOLOGY},
