   Valta, Katerina and Tsolakis, Apostolos C. and Karakostas, Anastasios},
Editor = {Solic, P and Nizetic, S and Rodrigues, JJPC and Perkovic, T and Catarinucci, L and Patrono, L and Gonzalez-De-Artaza, DLD},
Title = {Enhancing Critical Raw Material Management in Construction: The Role of
   Building Information Modelling and Life Cycle Assessment},
Booktitle = {2024 9TH INTERNATIONAL CONFERENCE ON SMART AND SUSTAINABLE TECHNOLOGIES,
   SPLITECH 2024},
