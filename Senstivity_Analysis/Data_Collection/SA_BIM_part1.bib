@article{ WOS:000391731900020,
Author = {Veiga, E. F. and Bulcao Neto, R. F.},
Title = {A Systematic Mapping of the Ontology-Based Modeling Phase of Context
   Life Cycle},
Journal = {IEEE LATIN AMERICA TRANSACTIONS},
Year = {2016},
Volume = {14},
Number = {10},
Pages = {4345-4350},
Month = {OCT},
Abstract = {The context life cycle demonstrates how context information moves around
   each phase into a system, from context modeling until context
   dissemination. The modeling phase turns context data gathered from
   sensors into a higher-level information, which in most cases is modeled
   through ontologies. Aiming at the building of a semantic context
   services infrastructure, this paper presents a systematic mapping of the
   state of art (2009-2015) about ontological context modeling-related
   services. The aim is to ground the development of those services,
   especially ontology-based context representation, storage and history,
   as part of a broad context management system. A systematic mapping
   protocol is described phase by phase and concludes with
   macro-architectural and general design principles, quality of design
   attributes, and open issues, all regarding the design of ontological
   context services.},
