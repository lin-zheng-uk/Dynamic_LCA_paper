Title = {A Semantic Hybrid Scheme for Efficient Building Facilities Management},
Booktitle = {CONSTRUCTION RESEARCH CONGRESS 2024: ADVANCED TECHNOLOGIES, AUTOMATION,
   AND COMPUTER APPLICATIONS IN CONSTRUCTION},
Year = {2024},
