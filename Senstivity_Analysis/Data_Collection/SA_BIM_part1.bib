@article{ WOS:000535448500001,
Author = {Keskin, Basak and Salman, Baris},
Title = {Building Information Modeling Implementation Framework for Smart Airport
   Life Cycle Management},
Journal = {TRANSPORTATION RESEARCH RECORD},
