@article{ WOS:000479230300005,
Author = {Ismail, Zul-Atfi},
Title = {An Integrated Computerised Maintenance Management System (I-CMMS) for
   IBS building maintenance},
Journal = {INTERNATIONAL JOURNAL OF BUILDING PATHOLOGY AND ADAPTATION},
Year = {2019},
Volume = {37},
Number = {3, SI},
Pages = {326-343},
Month = {JUN 10},
Abstract = {Purpose The purpose of this paper is to focus on the development of an
   integrated computerised maintenance management system to improve the
   information storage of design and construction, diagnostic and defect
   risk assessments on IBS building through the integration of building
   information modelling (BIM). Design/methodology/approach The methodology
