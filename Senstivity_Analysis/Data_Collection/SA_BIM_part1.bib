@article{ WOS:000458222600028,
Author = {Sousa, Vitor and Silva, Cristina Matos and Meireles, Ines},
Title = {Performance of water efficiency measures in commercial buildings},
Journal = {RESOURCES CONSERVATION AND RECYCLING},
Year = {2019},
Volume = {143},
Pages = {251-259},
Month = {APR},
Abstract = {Water efficiency certification systems ease communication between all
   interested parties of building projects but do not provide information
   about the life cycle costs of water efficiency measures nor warrant a
   level of water efficiency performance. The present study aims at
   understanding of the effective performance of water efficiency solutions
   in commercial buildings by reporting and analyzing a specific case study
   - Colombo Shopping Center. The water efficiency measures implemented in
   Colombo Shopping Center were the following: (i) in 2008, the restrooms
   fixtures were replaced by more efficient models; (ii) in 2011, a
   rainwater harvesting system (RWHS) was introduced for supplying the
   cogeneration plant; and (iii) also in 2011, a greywater reuse system
   (GWS) was installed to use the cooling towers purged water to supply the
   toilets of four restrooms. The methodology included collecting the water
   metering records and the accounting archives since 2006, assessing the
   water savings and evaluating the partial life cycle costs of each
   measure. The partial life cycle cost analysis covers the acquisition and
   the first years of the operation and maintenance stages. The restroom
   fixtures present the highest annual water savings along with longest
   payback period (> 15 years), since both the investment and maintenance
   costs are high. The estimated payback period for the RWHS is roughly 10
   years. The GWS was paid in just over 2 years and has annual water
   savings similar to the RWHS.},
