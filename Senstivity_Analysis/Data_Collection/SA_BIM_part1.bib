@article{ WOS:001010760600001,
Author = {Deng, Xueyuan and Lu, Kun},
Title = {Multi-level assessment for embodied carbon of buildings using
   multi-source industry foundation classes},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2023},
Volume = {72},
Month = {AUG 1},
Abstract = {The building industry produces a large number of carbon emissions,
   causing the intensification of the greenhouse effect. Utilizing building
   information modeling (BIM) is an excellent way to assess embodied
   carbon, but the incompatibility from multi-resource heterogeneous BIM
   software ob-structs this process. Therefore, this paper proposes a
   six-step methodology for calculating embod-ied carbon based on industry
   foundation classes (IFC). This method's multi-level calculation also
   provides a transparent process under the building, domain, element, bill
   of quantity, and re-source level, which is beneficial for impartial
   carbon audits. Based on this methodology, an inno-vative IFC tool,
   NMBIM, is developed to automate this process. Furthermore, this research
   uses a campus building with comprehensive spatial and temporal
   boundaries to verify the above methodology and tool. The life cycle
   embodied carbon of the case building is simulated by di-rectly using
   existing multi-resource heterogeneous BIM models at the design stage,
   reducing from 837.23 kgCO2-e/m2 to 727.08 kgCO2-e/m2. This study extends
   the BIM-based embodied carbon assessment from the single source to
   multi-source heterogeneity. The high-quality data-sharing solution
   effectively reuses heterogeneous BIM models to quickly assess and
   optimize embodied carbon in multi-participant collaborative works.},
