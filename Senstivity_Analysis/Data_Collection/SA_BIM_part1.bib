@inproceedings{ WOS:000380490000068,
Author = {Gudnason, G. and Katranuschkov, P. and Scherer, R. J. and Balaras, C. A.},
Editor = {Mahdavi, A and Martens, B and Scherer, R},
Title = {Framework for sharing and re-use of domain data in whole building energy
   simulation},
Booktitle = {EWORK AND EBUSINESS IN ARCHITECTURE, ENGINEERING AND CONSTRUCTION 2014},
Year = {2015},
Pages = {495-502},
Note = {PROCEEDINGS OF THE 10TH EUROPEAN CONFERENCE ON PRODUCT AND PROCESS
   MODELLING (ECPPM 2014), Department of Building Physics and Building
   Ecology of the Vienna Universi, Vienna, AUSTRIA, SEP 17-19, 2014},
Abstract = {Tools for whole Building Energy Simulation and Life Cycle Analyses are
   increasingly becoming more popular (and mandatory) in building design
   and facility management for analyzing building energy performance and
   environmental impact. A major barrier in deployment of these tools has
   been the lack of interoperability as their underlying data and
   mathematical models have escalated in complexity and consequently the
   demand for accurate precision data. The required analyses data exists,
   but in distributed, heterogonous data sources that require substantial
   manual preparation for use that is often the source of inaccurate and
   incomplete data model configuration. This paper introduces a Simulation
   Resource Framework facilitating access to energy-related resource
   libraries and design decision templates capturing many of the essential
   information for simulation model configuration e.g. weather data,
   occupancy activities, construction types and material properties. Such
   templates and resources may be especially helpful during early design
   stages where knowledge about the building's performance and components
   is limited and fundamental decisions need to be taken. The paper
   describes the overall concept and development approach of the framework
   and its integration in a broader Multi-Model environment enabling
   interoperability of BIM and any needed non-BIM resources in a virtual
   energy lab platform.},
