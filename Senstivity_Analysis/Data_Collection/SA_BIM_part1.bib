@article{ WOS:000425730400004,
Author = {Zhang, Junqiang and Wu, Chonglong and Wang, Yuzhu and Ma, Yan and Wu,
   Yong and Mao, Xiaoping},
Title = {The BIM-enabled geotechnical information management of a construction
   project},
