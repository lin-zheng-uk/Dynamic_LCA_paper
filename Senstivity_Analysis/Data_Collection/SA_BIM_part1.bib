@article{ WOS:001046378400001,
Author = {Witicovski, Lilian Cristine and Tavares, Sergio Fernando},
Title = {Integration Building Information Modeling (BIM) and Life Cycle
   Assessment (LCA)},
Journal = {REVISTA DE GESTAO E SECRETARIADO-GESEC},
Year = {2023},
Volume = {14},
Number = {8},
Pages = {12469-12480},
Abstract = {In an integrated project, the main decisions are evaluated by the
   project team in the conceptual phase. With the large volume of
   information needed to carry out a project in the civil construction
   industry, the demand for more rational and better performing processes
   is growing. As a strategic role, the BIM model combined with the Life
   Cycle Assessment (LCA) help in the selection of materials and
   construction techniques for more sustainable projects. The analyzes
   generated make it possible for companies and architectural design
   offices to decide which strategies to adopt to reduce environmental
   impacts. There is a wide variety of software available on the market to
   assist in these studies. This article presents an overview of Brazilian
   scientific productions, within the range of 2010 to 2017, that use
   environmental analysis linked to BIM. It analyzes the evolutionary
   profile of the scientific production of simulations and the exchange
   between the exchange of information to arrive at environmental analyses.
   It was possible to conclude that BIM+LCA simulations and information
   exchange between applications and users require detailed planning and
   fundamental modifications in the design process. Another point noted is
   that, in Brazil, there is no information generated by national databases
   for a complete LCA, which is fundamental in decision-making during the
   life cycle of the building.},
