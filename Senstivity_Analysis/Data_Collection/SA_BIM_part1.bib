@article{ WOS:000459298000001,
Author = {Spinnraeker, E. and Pauen, N. and Schnitzler, A. and Frisch, J. and van
   Treeck, C.},
Title = {Webtool for riskbased-probabilistic life cycle cost analysis based on
   digital building models - <i>BIM<sub>2p</sub>LCC</i>},
Journal = {BAUINGENIEUR},
Year = {2019},
Volume = {94},
Number = {2},
Pages = {37-44},
Month = {FEB},
Abstract = {In comparison to other manufacturing industries, labor productivity per
   employed person has been stagnating for years. Despite the increasing
   use of digital planning methods in practice, the reasons include
   insufficient standardization and an associated lack of automation of
   processes and calculations. As a result, selective and deterministic
   building cost calculations still exist, which usually take place only at
   late planning stages. Hence, the inadequate identification and
   communication of opportunities and risks as well as the usually missing
   consideration of the usage costs preclude a holistic view and
   optimization of buildings.
   The following web-based tool BIM2pLCC (BIM to probabilistic
   Life-Cycle-Costing) provides a solution for the efficient calculation of
   probabilistic life cycle cost analyzes at different levels of
   information. The chosen implementation allows cost calculations to be
   performed either using three input parameters and statistical enrichment
   of (cost-) characteristics, or on the basis of BIM models with different
   information content. Therefore, a continuous probabilistic life cycle
   cost analysis can be integrated into the planning process and planning
   decisions can be made with a profound information background.},
