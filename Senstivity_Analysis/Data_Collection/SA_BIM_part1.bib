   AzariJafari, Hessam, MIT, 77 Massachusetts Ave, Cambridge, MA 02139 USA.},
DOI = {10.1016/j.scs.2021.102728},
EarlyAccessDate = {MAR 2021},
Article-Number = {102728},
ISSN = {2210-6707},
EISSN = {2210-6715},
Keywords = {Building information modeling; Life cycle assessment; Integration; Level
