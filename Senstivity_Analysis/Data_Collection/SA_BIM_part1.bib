@inproceedings{ WOS:000356870700049,
Author = {Tang, L. C. M. and Cho, S. Y. and Xia, L.},
Book-Group-Author = {IEEE},
Title = {Intelligent HVAC Information Capturing System for Smart Building
   Information Modelling},
Booktitle = {2013 5TH INTERNATIONAL CONFERENCE ON POWER ELECTRONICS SYSTEMS AND
   APPLICATIONS (PESA)},
Year = {2013},
Note = {5th International Conference on Power Electronics Systems and
   Applications (PESA), Hong Kong, PEOPLES R CHINA, DEC 11-13, 2013},
Abstract = {Building Information Modelling (BIM) has implications for all processes
   and activities related to construction supply chain and can, thus, make
   significant contributions to lean construction process. The existing
   dimensions of BIM not only attend to most aspects of the construction
   work and processes, but the technology also has the potential to add
   further dimensions responding to other existing or future challenges.
   This paper will look into ways in which Artificial Neural Network
   (ANN)-COBie can help architects and engineers to perform HVAC analysis
   with the support of a BIM platform. Other applications e. g. HVAC load
