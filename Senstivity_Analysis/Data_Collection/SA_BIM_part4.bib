Title = {Evolving Relationship Structures in Multi-sourcing Arrangements: The
   Case of Mission Critical Outsourcing},
Booktitle = {GLOBAL SOURCING OF INFORMATION TECHNOLOGY AND BUSINESS PROCESSES},
