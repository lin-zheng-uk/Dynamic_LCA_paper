@article{ WOS:000248233300004,
Author = {Arias, P. and Armesto, J. and Di-Capua, D. and Gonzalez-Drigo, R. and
   Lorenzo, H. and Perez-Gracia, V.},
Title = {Digital photogrammetry, GPR and computational analysis of structural
   damages in a mediaeval bridge},
