@article{ WOS:000541081900001,
Author = {Wuni, Ibrahim Yahaya and Shen, Geoffrey Qiping and Osei-Kyei, Robert},
Title = {Quantitative evaluation and ranking of the critical success factors for
   modular integrated construction projects},
Journal = {INTERNATIONAL JOURNAL OF CONSTRUCTION MANAGEMENT},
