@article{ WOS:000679816000001,
Author = {Akanmu, Abiola A. and Anumba, Chimay J. and Ogunseiju, Omobolanle O.},
Title = {TOWARDS NEXT GENERATION CYBER-PHYSICAL SYSTEMS AND DIGITAL TWINS FOR
   CONSTRUCTION},
