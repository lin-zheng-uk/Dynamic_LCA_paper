@article{ WOS:000573833600001,
Author = {Barrows, Caitlynn M. L. and Wu, Danielle and Farach-Carson, Mary C. and
   Young, Simon},
Title = {Building a Functional Salivary Gland for Cell-Based Therapy: More than
   Secretory Epithelial Acini},
