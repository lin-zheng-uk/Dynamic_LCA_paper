@inproceedings{ WOS:000556337401049,
Author = {Samarskaya, Nadezhda A. and Gorodnova, Natalia V. and Skipin, Dmitry L.
   and Peshkova, Anastasiya A.},
Editor = {Soliman, KS},
Title = {Improving the Standard of Living of the Population under the Modern
   Economic Conditions},
Booktitle = {VISION 2025: EDUCATION EXCELLENCE AND MANAGEMENT OF INNOVATIONS THROUGH
   SUSTAINABLE ECONOMIC COMPETITIVE ADVANTAGE},
Year = {2019},
Pages = {881-897},
Note = {34th International-Business-Information-Management-Association (IBIMA)
   Conference, Madrid, SPAIN, NOV 13-14, 2019},
Organization = {Int Business Informat Management Assoc},
Abstract = {The course taken by the Russian state to form and develop an innovative
   economy should significantly expand the development potential of
   society, which is reflected in the efficiency and quality of economic
   processes and characterizes the social orientation of the country.
   Improving the level and standard of life of the society as a whole plays
   the corresponding role in the model of economic development where the
   innovation component dominates. As evidenced in practice, the current
   state of the economy of any country in the world is characterized
   primarily by the level and standard of citizens' lives. Indicators of
   the population's standard of life are the most important indicators for
   assessing the real socio-economic effects of the transformations and the
   degree of social tension in society. Achieving the highest possible
   standard of life for the country's population is a priority task for the
   socially-oriented economy of modern Russia.Currently, the executive
   authorities in Russia do not draw a clear distinction between the
   notions of the level and standard of life of the population, presenting
   official information on the group of ``The Level of Life{''} indicators.
   The problem of improving the standard of life of the population is the
   most important strategic task of the development of the society at the
   current stage, which has determined the relevance of the research topic.
   The study of the concept of ``quality of life{''} in the modern economy
   of the Russian Federation allowed the authors to analyze the elements of
   an economic and social nature. Based on the available statistical data,
   the authors proposed directions to improve the quality of life of the
