}

@CONFERENCE{Altaf2020,
	author = {Altaf, Muhammad and Alaloul, Wesam Salah and Musarat, Muhammad Ali and Bukhari, Hamna and Saad, Syed and Ammad, Syed},
	title = {BIM Implication of Life Cycle Cost Analysis in Construction Project: A Systematic Review},
	year = {2020},
