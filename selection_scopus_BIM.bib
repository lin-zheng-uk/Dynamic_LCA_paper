	doi = {10.1016/j.proeng.2017.08.068},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85030476329&doi=10.1016%2fj.proeng.2017.08.068&partnerID=40&md5=69d0ad66f477601583bd4427d5dc859f},
